				<div class="dropdown-divider"></div>
		    </div>
		</div>
	</div>
	<!-- END PROFILE MODAL -->

	<!-- RE-AUTHENTICATE MODAL-->
	<div id="authenticateModalCont" class="modal fade authenticateModal authenticate-modal-md" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" data-backdrop="static">
		<div class="modal-dialog modal-md">
		   	<div id="authenticateModal" class="modal-content">
                <button id="exitLoginModal" type="button" class="close text-right" data-dismiss="modal" aria-label="Close"><i data-feather="x"></i>
                </button>
                <h3 class="text-center">Re-authenticate</h3>
                <div class="row justify-content-center">
                	<div id="authenticateAlert" class="alert alert-warning col col-lg-10 animated fadeIn" role="alert">
					  This action requires that you recenlty signed in. Please enter your credentials to proceed.
					</div>
					<div class="col col-lg-10">
						<label for="loginEmail">Email Address</label>
						<input type="email" class="form-control form-control-lg" id="loginEmail" aria-describedby="emailHelp" placeholder="jared@piedpiper.com" autocomplete="off">
						<p id="loginEmailError" class="formError"></p>
					</div>
					<div class="col col-lg-10">
					    <label for="loginPassword">Password</i></label>
    					<input type="password" class="form-control form-control-lg" id="loginPassword" placeholder="Password" autocomplete="off">
    					<p id="loginPasswordError" class="formError"></p>
					</div>
					<div id="loginBtnCont" class="col col-lg-10">
					  	<a id="loginBtn" class="btn col col-lg-12" href="#">Log In</a>
					</div>
					<div id="authenticateError" class="alert alert-danger col-lg-10 animated bounceIn" role="alert">
					  <p id="authenticateErrorMsg" class="text-center"></p>
					</div>
                </div>
            </div>
		</div>
	</div>
	<!-- END RE-AUTHENTICATE MODAL -->

	<!-- CHANGE PASSWORD MODAL -->
	<div id="changePasswordModalCont" class="modal fade changePasswordModal changePassword-modal-md" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" data-backdrop="static">
		<div class="modal-dialog modal-md">
		   	<div id="changePasswordModal" class="modal-content">
                <button id="exitLoginModal" type="button" class="close text-right" data-dismiss="modal" aria-label="Close"><i data-feather="x"></i>
                </button>
                <h3 class="text-center">Change Password</h3>
                <div class="row justify-content-center">
    				<div class="col col-lg-8">
					    <label for="newPassword">New Password</i></label>
    					<input type="password" class="form-control form-control-lg" id="newPassword" placeholder="New password" autocomplete="off">
					</div>
					<div class="col col-lg-8">
					    <label for="confirmPassword">Confirm Password</i></label>
    					<input type="password" class="form-control form-control-lg" id="confirmPassword" placeholder="Confirm password" autocomplete="off">
					</div>
					<div id="changePasswordBtnCont" class="col col-lg-8">
					  	<a id="changePasswordBtn" class="btn col col-lg-12" href="#">Change Password</a>
					</div>
					<div id="changePasswordErrorMessage" class="col col-lg-8 alert alert-danger animated bounceIn" role="alert"></div>
				</div>
            </div>
		</div>
	</div>
	<!-- END CHANGE PASSWORD MODAL -->
	
	<!-- COLOR THEME MODAL -->
	<div id="themeModal" class="modal fade colorModal modal-md" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
		<div class="modal-dialog modal-md">
		   	<div id="changeThemeModal" class="modal-content">
                <button id="exitLoginModal" type="button" class="close text-right" data-dismiss="modal" aria-label="Close"><i data-feather="x"></i>
                </button>
                <h3 class="text-center">Change Theme</h3>
                <p class="text-center">Spice your chat up with some fancy colors!</p>
                <div id="colorDiv" class="row justify-content-center"></div>
            </div>
		</div>
	</div>
	<!-- END COLOR THEME MODAL -->

	<!-- EMAIL MODAL -->
	<div id="emailModal" class="modal fade emailModal modal-md" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
		<div class="modal-dialog modal-md">
		   	<div id="sendEmailModal" class="modal-content">
                <button id="exitLoginModal" type="button" class="close text-right" data-dismiss="modal" aria-label="Close"><i data-feather="x"></i>
                </button>
                <iframe id="iframe" name="hidden"></iframe>
                <form id="emailForm" action="/dashboard" method="post" target="hidden">
	                <div class="row justify-content-center">
						<img id="emailAvatar" src="/img/avatar.png" alt="profile avatar">
		            </div>
	                <h3 id="sendEmailName" class="text-center"></h3>
	                <div id="emailDiv" class="row justify-content-center">
	    				<input type="email" class="form-control col col-lg-10" id="sendEmailAddress" name="toAddress" readonly>
	    				<input type="text" class="form-control col col-lg-10" id="fromEmailAddress" name="fromAddress">
					</div>
					<div id="emailSubject" class="row justify-content-center">
	    				<input type="text" class="form-control col col-lg-10" id="sendEmailSubject" name="emailSubject" placeholder="Subject" autocomplete="off">
					</div>
					<div id="emailContentDiv" class="row justify-content-center">
	    				<textarea class="form-control col col-lg-10" id="sendEmailContent" name="emailContent" rows="3" autocomplete="off"></textarea>
					</div>
					<div id="sendEmailBtnCont" class="row justify-content-center">
						<button id="sendEmailBtn" class="btn col col-lg-10" type="submit" href="#">SEND EMAIL</button>
					</div>
				</form>
            </div>
		</div>
	</div>
	<!-- End EMAIL MODAL -->

	<!-- NEW PROJECT MODAL -->
	<div id="newProjectModal" class="modal fade newProjectModal modal-md" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" data-backdrop="static">
		<div class="modal-dialog modal-md">
		   	<div id="newProjectModalCont" class="modal-content">
                <button id="exitLoginModal" type="button" class="close text-right" data-dismiss="modal" aria-label="Close"><i data-feather="x"></i>
                </button>
                <h3 class="text-center">New Project</h3>
                <div id="newProjectCont" class="row col-lg-10 offset-lg-1">
                	<p class="text-left newProjectLabel">Project name</p>
	    			<input type="text" class="form-control newProjectInput col col-lg-12" id="newProjectName" name="newProjectName" placeholder="Find water on Mars" autocomplete="off">
	    			<p id="projectNameError"></p>
                </div>
                <div class="row col-lg-10 offset-lg-1">
                	<p class="newProjectLabel">Project ID</p>
                	<p id="newProjectId" class="col-lg-12"></p>
                </div>
                <div class="row col-lg-10 offset-lg-1">
                	<p class="newProjectLabel">Project description</p>
	    			<textarea class="form-control newProjectInput col col-lg-12" id="newProjectDesc" name="newProjectDesc" rows="3" autocomplete="off" placeholder="Write a short description about the project..."></textarea>
	    			<p id="projectDescError"></p>
				</div>
				<div class="row col-lg-10 offset-lg-1">
					<p class="newProjectLabel col-lg-12">Select friends to join project</p>
					<input type="text" class="form-control newProjectInput col col-lg-12" id="newProjectSearch" name="newProjectSearch" placeholder="Search for a friend..." autocomplete="off">
				</div>
				<div id="newProjectFriends" class="row col-lg-10 offset-lg-1">
				</div>
                <div class="modal-footer">
                	<button type="button" id="cancelProject" class="btn" data-dismiss="modal">CANCEL</button>
		        	<button type="button" id="createProject" class="btn">CREATE PROJECT</button>
		    	</div>
            </div>
		</div>
	</div>
	<!-- END NEW PROJECT MODAL -->


	<!-- SNACKBAR -->
	<div id="snackbar"></div>
	<!-- END SNAKCBAR -->
	</section>
</body>
</html>